</servlet-mapping>
<servlet-mapping>
<servlet-name>LoginServlet</servlet-name>
<url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>AddressServlet</servlet-name>
<url-pattern>/AddressServlet</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>BookServlet</servlet-name>
